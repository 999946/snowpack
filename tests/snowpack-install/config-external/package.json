{
  "private": true,
  "version": "1.0.0",
  "name": "@snowpack/test-config-external",
  "description": "Handle external packages",
  "scripts": {
    "testcmd": "snowpack"
  },
  "snowpack": {
    "installOptions": {
      "externalPackage": [
        "mock-test-package-b"
      ]
    },
    "install": [
      "mock-test-package-a"
    ],
    "buildOptions": {
      "minify": false
    }
  },
  "dependencies": {
    "mock-test-package-a": "^1.0.0",
    "mock-test-package-b": "^1.0.0"
  },
  "devDependencies": {
    "snowpack": "*"
  }
}
